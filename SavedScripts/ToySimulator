function jumpEvery5Seconds(character)
    local humanoid = character:FindFirstChild("Humanoid")
    if not humanoid then
        return -- –≤—ã—Ö–æ–¥–∏–º –∏–∑ —Ñ—É–Ω–∫—Ü–∏–∏, –µ—Å–ª–∏ –Ω–µ—Ç –∫–æ–º–ø–æ–Ω–µ–Ω—Ç–∞ "Humanoid"
    end
    
    local jumpDelay = 3


        while _G.AntiAfk == true do
            wait(jumpDelay)
            humanoid.Jump = true
        end
    end


                                            local player = game.Players.LocalPlayer
                                            local character = player.Character or player.CharacterAdded:Wait()

function AutoEquipPets()
    while _G.AutoEquipPets == true do
        game:GetService("ReplicatedStorage").Remotes.Inventory:FireServer("EquipBestToys")
        wait(30)
        
         end
    end

function AutoHatch()
    while _G.AutoHatch == true do
        game:GetService("ReplicatedStorage").Remotes.OpenCrate:FireServer(_G.EggWorld, _G.EggArea, _G.EggTier)
        wait(0.5)
         end
    end

function TPSystemKey()
    while _G.TPSystemKey == true do
        _G.TPSystemKey = false
        wait(0.01)
         end
    end

function HitSpeed()
    while _G.HitSpeed == true do
        game:GetService("ReplicatedStorage").Remotes.Hit:FireServer()
        wait(0.1)
         end
    end

function AutoDeletePets()
    while _G.ReallyDestroy == true do
        local Event = game:GetService("ReplicatedStorage").Remotes.Inventory
        Event:FireServer("DeleteToys", {
            [1] = game:GetService("Players").LocalPlayer.Toys[_G.EnterPetDelete]
        })
    wait(0.1)
    end
end

function AutoFarm()
    while _G.AutoFarm == true do
        local Players = game:GetService("Players")
        local ReplicatedStorage = game:GetService("ReplicatedStorage")
        local player = Players.LocalPlayer

    while not player.Character or not player.Character:IsDescendantOf(workspace) do
    player.CharacterAdded:Wait()
    end

            local target = nil
            local maxdistance = math.huge

    for _, v in pairs(workspace.Misc.Items:GetChildren()) do
    if not v:FindFirstChild("ClickDetector") then continue end

    local dist = (v.PrimaryPart.Position - player.Character.HumanoidRootPart.Position).Magnitude

    if dist > maxdistance then continue end

    target = v
    maxdistance = dist
    end

    if target then
    fireclickdetector(target.ClickDetector)
    end
    wait(0.01)

    ReplicatedStorage.Remotes.Hit:FireServer()
         end
    end

function AutoCollectDrop()
    while _G.AutoDrop == true do
        local character = game:GetService("Players").LocalPlayer.Character
        if character and character:FindFirstChild('HumanoidRootPart') then
            for i, v in pairs(workspace:GetDescendants()) do
                if v.Name == 'Crystals' or v.Name == 'CyberCoins' or v.Name == 'Coins' then
                    v.CFrame = character.HumanoidRootPart.CFrame + Vector3.new(0, 8, 0) -- –∏–∑–º–µ–Ω–µ–Ω–æ: –¥–æ–±–∞–≤–ª–µ–Ω –≤–µ–∫—Ç–æ—Ä –≤–≤–µ—Ä—Ö
                end
            end
        end
        wait(0.3) -- –¥–æ–±–∞–≤—å—Ç–µ —ç—Ç–æ—Ç –≤—ã–∑–æ–≤, –µ—Å–ª–∏ –Ω–µ–æ–±—Ö–æ–¥–∏–º–æ –∞—Å–∏–Ω—Ö—Ä–æ–Ω–Ω–æ–µ –ø–æ–≤–µ–¥–µ–Ω–∏–µ
    end
end


-- –î–û–ü–û–õ–ù–ò–¢–ï–õ–¨–ù–û–ï ( INF.JUMP ) -- 

game:GetService("UserInputService").InputBegan:Connect(function(input, gameProcessed) 
    if not gameProcessed and input.KeyCode == Enum.KeyCode.Space and isInfinityJumpEnabled then 
        game.Players.LocalPlayer.Character.Humanoid:ChangeState(Enum.HumanoidStateType.Jumping) 
    end 
end) 

