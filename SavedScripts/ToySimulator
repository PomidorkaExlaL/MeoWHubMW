local TierDropdown = EggTab:AddDropdown({
    Name = "Select Tier",
    Default = "tier_1",
    Options = {"tier_1", "tier_2", "tier_3"},
    Callback = function(Value)
        _G.EggTier = Value
    end
})

local AreaDropdown = EggTab:AddDropdown({
    Name = "Select Area",
    Default = "Town",
    Options = {"Town", "Dinosaur Kingdom", "Farm Life"},
    Callback = function(Value)
        _G.EggArea = Value
    end
})

local WorldDropdown = EggTab:AddDropdown({
    Name = "Select World",
    Default = "Earth",
    Options = {"Earth", "Cyberverse"},
    Callback = function(Value)
        _G.EggWorld = Value

        if Value == "Earth" then
            AreaDropdown:Refresh({"Desert Adventure", "Dinosaur Kingdom", "Aqua Land", "Fantasy Forest", "Farm Life", "Frosty Kingdom", "Jungle Expedition", "Mountain Expedition", "Pirate Cove", "Town", "Cerberus Room", "Hydra Room", "The Portal", "VIP"}, true)
        elseif Value == "Cyberverse" then
            AreaDropdown:Refresh({"Cyber Town", "Robo Road", "Cyber Adventure", "Robo Expedition", "Cyborg Cerberus Room"}, true)
        end


    end
})

local AreaDropdownTP = TeleportTab:AddDropdown({
        Name = "Select Area",
        Default = "Town",
        Options = {"Town", "Dinosaur Kingdom", "Farm Life"},
        Callback = function(Value)
            _G.TpArea = Value
        end
})

local WorldDropdownTP = TeleportTab:AddDropdown({
    Name = "Select World",
    Default = "Earth",
    Options = {"Earth", "Cyberverse"},
    Callback = function(Value)
         _G.TpWorld = Value
         if Value == "Earth" then
             AreaDropdownTP:Refresh({"Desert Adventure", "Dinosaur Kingdom", "Aqua Land", "Fantasy Forest", "Farm Life", "Frosty Kingdom", "Jungle Expedition", "Mountain Expedition", "Pirate Cove", "Town", "Cerberus Room", "Hydra Room", "The Portal", "VIP"}, true)
         elseif Value == "Cyberverse" then
             AreaDropdownTP:Refresh({"Cyber Town", "Robo Road", "Cyber Adventure", "Robo Expedition", "Cyborg Cerberus Room"}, true)
         end
    end
})

